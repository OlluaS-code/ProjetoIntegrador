{
  "name": "gestao_rh",
  "version": "1.0.0",
  "description": "API Backend para Sistema de RH (SIRH) Multi-Tenant em Clean Architecture.",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "npx @biomejs/biome lint src",
    "format": "npx @biomejs/biome format --write src",
    "check": "npm run format && npm run lint"
  },
  "keywords": [
    "typescript",
    "express",
    "mysql",
    "clean-architecture",
    "multi-tenant"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "jest": "^30.2.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
